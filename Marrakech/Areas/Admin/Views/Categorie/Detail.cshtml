@model Categorie
@{
    ViewData["Title"] = "Detail";
}


<br />

<h2 class="text-info">Détail Catégorie</h2>
<br />

<div class="whiteBg">
    @Html.HiddenFor(m => Model.Id)
    <div class="form-group row">
        <div class="col-2">
            <label asp-for="Id" class="col-form-label"></label>
        </div>
        <div class="col-5">
            <input asp-for="Id" readonly class="form-control" />
        </div>
    </div>
    <div class="form-group row">
        <div class="col-2">
            <label asp-for="Name" class="col-form-label"></label>
        </div>
        <div class="col-5">
            <input asp-for="Name" readonly class="form-control" />
        </div>
    </div>
    <div class="form-group row">
        <div class="col-5 offset-2">
            <partial name="_DetailEtRetourpartial"  model="@Model.Id"/>

        </div>
    </div>
</div>








